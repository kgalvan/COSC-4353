package fibonacci;

public class Fibonacci{

    public int recursive(int n){
        if(n == 0)
            return 1;
        else if(n == 1)
            return 1;
        else
            return recursive(n - 1) + recursive(n - 2);
    }

    public int iterative(int n){
        int prev = 1;
        int next = 1;
        int result = 1;
        for (int i = 0; i < n-1; i++) {
            result = prev+next;
            prev = next;
            next = result;
        }
        return result;
    }
}

