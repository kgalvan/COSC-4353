package fibonacci;

import java.math.BigInteger;

public class FibonacciIterativeSolution{

    public BigInteger compute(int number){  //Venkat: position instead of number
        BigInteger previous = new BigInteger("1"); //Venkat: Move one of these three to inside for
        BigInteger next = new BigInteger("1"); 
        BigInteger result = new BigInteger("1"); 

        for (int i = 0; i < number-1; i++) {
            result = previous.add(next);
            previous = next;
            next = result;
        } //Venkat:  a blank line after this, please
        return result;
    }
}

