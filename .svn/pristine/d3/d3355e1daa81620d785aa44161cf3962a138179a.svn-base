package fibonacci;

import java.math.BigInteger;

public class FibonacciIterativeSolution implements FibonacciSolution{

    @Override
    public BigInteger compute(int position){
        BigInteger previous = new BigInteger("1"); //Venkat: Move one of these three to inside for
        BigInteger next = new BigInteger("1"); 
        BigInteger result = new BigInteger("1");

        for (int i = 0; i < position-1; i++) {
            result = previous.add(next);
            previous = next;
            next = result;
        }

        return result;
    }

}

